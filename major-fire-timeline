<script >

 
 var timelineData = [{"id":1,"year":"Feb 12, 2019","headline":"Feb 12, 2019","desc":"17 killed in a fire at a hotel in central Delhi’s congested Karol Bagh area.","images":"#","links":null},{"id":2,"year":"Feb 7, 2019","headline":"Feb 7, 2019","desc":"Scores of patients and staff at Noida’s Metro Hospital and Heart Institute had to be evacuated after a major fire.","images":"#","links":" "},{"id":3,"year":"Jan 30, 2019","headline":"Jan 30, 2019","desc":"Four people were injured in a fire at a chemical factory in Okhla Phase-I in southeastern Delhi.","images":"#","links":null},{"id":4,"year":"Nov 19, 2018","headline":"Nov 19, 2018","desc":"Four people were killed and one person was injured after a fire broke out at a factory in central Delhi’s Karol Bagh.","images":"#","links":null},{"id":5,"year":"May 29, 2018","headline":"May 29, 2018","desc":"A massive blaze, which fire officials said was of “highest magnitude”, broke out at a godown in south Delhi’s Malviya Nagar. No casualty was reported.","images":"#","links":null},{"id":6,"year":"April 23, 2018","headline":"April 23, 2018","desc":"At least 300 shanties were gutted after a major fire broke out in Shahdara’s MS Park. A girl was killed in the fire.","images":"#","links":null},{"id":7,"year":"April 13, 2018","headline":"April 13, 2018","desc":"Four members of a family, including two minor children, were killed in a major fire in Delhi’s Kohat Enclave.","images":"#","links":null},{"id":8,"year":"Jan 20, 2018","headline":"Jan 20, 2018","desc":"At least 10 women and seven men were charred to death in a fire that broke out in a firecracker unit in northwest Delhi’s Bawana industrial area.","images":"#","links":null},{"id":9,"year":"Nov 20, 2011","headline":"Nov 20, 2011","desc":" Fourteen people died and over 30 were injured when a fire broke out at a community function of eunuchs in Nandnagari in east Delhi.","images":"#","links":null},{"id":10,"year":"June 13, 1997","headline":"June 13, 1997","desc":"A fire broke out at the Uphaar theatre during the screening of Bollywood film ‘Border’, killing 59 people and injuring over 100.","images":"#","links":null}];
  
    // var array = $.makeArray(timelineData);
    var rCount = timelineData.length - 1;
    // console.log(array);

for ( var im = 0; im <= rCount; im++ ) {
    $("#dates").append('<li><a href="#'+ timelineData[im].id+'">'+ timelineData[im].year +'</a></li>')
}

for ( var m = 0; m <= rCount; m++ ) {
    $("#issues").append('<li id="'+ timelineData[m].id +'" class=""><h1>'+ timelineData[m].year+'</h1><p>'+ timelineData[m].desc +'</p></li>')
}


/**Timeline**/

$(function(){
    $().timelinr({
      orientation:  'vertical',
      issuesSpeed:  300,
      datesSpeed:   100,
      arrowKeys:    'true',
      startAt:      1
    })
  });


  /** pre-loader **/
  $(document).ready(function() {
 
 // Fakes the loading setting a timeout
   setTimeout(function() {
       $('body').addClass('loaded');
   }, 2500);

});

$('.clickme').click(function(){
    $('.body').css('display','none');
  });


</script>

